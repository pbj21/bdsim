p1: placement, geometryFile="gdml:gdmls/atf2_tunnel.gdml",
	       z=5*m;


d1: drift, l=1*m;
l1: line=(d1);
use, l1;

beam, particle="e-",
      energy=1.3*GeV;

! we turn off general gdml preprocessing but turn
! on schema preprocessing explicitly
option, preprocessGDML = 0,
	preprocessGDMLSchema = 1;